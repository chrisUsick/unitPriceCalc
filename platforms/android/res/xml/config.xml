<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cuDev.unitPrice" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
